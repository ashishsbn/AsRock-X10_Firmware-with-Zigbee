config wifi-device 'wifi0'
        option type 'qcawifi'
        option channel 'auto'
        option hwmode '11ng'
	option htmode 'HT40'
	option txpower_ptg '0'

config wifi-iface 'ath0'
        option device 'wifi0'
        option network 'lan'
        option mode 'sta'
	option extap '1'
        option ssid 'ASRock'
        option encryption 'psk2+aes'
        option key '12345678'
	option wps_pbc '1'

config wifi-device 'wifi1'
        option type 'qcawifi'
        option channel 'auto'
        option hwmode '11ac'
	option htmode 'HT80'
        option txpower_ptg '0'

config wifi-iface 'ath1'
        option device 'wifi1'
        option network 'lan'
        option mode 'sta'
	option extap '1'
        option ssid ''
        option encryption 'psk2+aes'
        option key '12345678'
	option wps_pbc '1'
