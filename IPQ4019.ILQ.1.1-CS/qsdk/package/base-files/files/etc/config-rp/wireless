config wifi-device 'wifi0'
        option type 'qcawifi'
        option channel 'auto'
        option hwmode '11ng'
	option htmode 'HT40'
	option txpower_ptg '0'

config wifi-iface 'ath0'
        option device 'wifi0'
        option network 'lan'
        option mode 'ap'
	option disabled '0'
        option ssid 'ASRock_RPT'
        option puren '0'
	option disablecoext '0'
	option protmode '1'
	option hidden '0'
	option preamble '0'
	option shortgi '1'
	option rts '2347'
	option dtim_period '3'
	option wmm '1'
	option encryption 'psk2+aes'
	option key '12345678'
	option wps_device_name 'X10'
	option wps_manufacturer 'ASRock'
	option wps_pbc '1'
	option wps_state '2'
	option mcastenhance '2'
	option athnewind '1'

config wifi-iface 'ath01'
        option device 'wifi0'
        option network 'lan'
        option mode 'sta'
	option extap '1'
        option ssid 'ASRock'
        option encryption 'psk2+aes'
        option key '12345678'
	option wps_pbc '1'

config wifi-device 'wifi1'
        option type 'qcawifi'
        option channel 'auto'
        option hwmode '11ac'
	option htmode 'HT80'
        option txpower_ptg '0'

config wifi-iface 'ath1'
        option device 'wifi1'
        option network 'lan'
        option mode 'ap'
	option disabled '0'
        option ssid 'ASRock_5G_RPT_5G'
        option puren '0'
	option disablecoext '0'
	option protmode '1'
	option hidden '0'
	option preamble '0'
	option shortgi '1'
	option rts '2347'
	option dtim_period '3'
	option wmm '1'
	option encryption 'psk2+aes'
	option key '12345678'
	option wps_device_name 'X10'
	option wps_manufacturer 'ASRock'
	option wps_pbc '1'
	option wps_state '2'
	option mcastenhance '2'
	option athnewind '1'

config wifi-iface 'ath11'
        option device 'wifi1'
        option network 'lan'
        option mode 'sta'
	option extap '1'
        option ssid ''
        option encryption 'psk2+aes'
        option key '12345678'
	option wps_pbc '1'
