config system
	option hostname	X10
	option timezone IST-5:30
	option city Asia/Kolkata
	option factory_default 1
	option lighting 1
	option lighting_level 100
	option log_file '/tmp/log/systemlog.txt'
	option log_size 4096
	option langurage 'en'
	option op_mode 0

config timeserver ntp
	list server	time.windiws.com	
	list server	time.nist.gov
	list server	time-nw.nist.gov
	list server	time-a.nist.gov
	list server	time-b.nist.gov
	option enable_server 0
